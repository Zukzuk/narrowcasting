{
    "name": "narrowcasting-app",
    "version": "3.0.6",
    "description": "Narrowcast your favorite media",
    "type": "module",
    "main": "dist/server/index.js",
    "engines": {
        "node": ">=16.14.0"
    },
    "author": "Dave Timmerman",
    "license": "MIT",
    "repository": {
        "type": "https",
        "url": "https://github.com/Zukzuk/narrowcasting.git"
    },
    "scripts": {
        "build": "mkdirp dist/public && cpx \"src/public/**/*\" dist/public && tsc",
        "dev": "node scripts/docker-dev.js",
        "commit": "cz",
        "release": "standard-version && git push --follow-tags origin main && npm run dev --APP_RUN=0",
        "docker:release": "node scripts/docker-release.js"
    },
    "dependencies": {
        "@skyra/jaro-winkler": "^1.1.1",
        "axios": "^1.7.7",
        "compression": "^1.7.5",
        "cors": "^2.8.5",
        "default-gateway": "^7.2.2",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "express-session": "^1.18.1",
        "sharp": "^0.33.5",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1"
    },
    "devDependencies": {
        "@types/node": "^22.9.1",
        "commitizen": "^4.3.1",
        "concurrently": "^9.1.0",
        "cpx": "^1.2.1",
        "cz-conventional-changelog": "^3.3.0",
        "husky": "^9.1.7",
        "mkdirp": "^3.0.1",
        "nodemon": "^3.1.7",
        "standard-version": "^9.5.0",
        "typescript": "5.6",
        "wait-on": "^8.0.1"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
